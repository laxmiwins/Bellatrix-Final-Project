<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="3">
  <test  name="ChromeTest">
	  <parameter name="browser-name" value="Chrome"></parameter>
    <classes>
      <class name="com.meteorite.base.TestBase"/>
    </classes>
  </test> <!-- Test -->
  
  <test  name="FirefoxTest">
	  <parameter name="browser-name" value="Firefox"></parameter>
    <classes>
      <class name="TestBase"/>
        <class name="com.meteorite.base.TestBase"/>
    </classes>
  </test> <!-- Test -->
  
  <test  name="InternetExplorerTest">
	  <parameter name="browser-name" value="InternetExplorer"></parameter>
    <classes>
      <class name="com.meteorite.base.TestBase"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
